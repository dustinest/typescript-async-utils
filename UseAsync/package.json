{
  "name": "react-useasync-hooks",
  "version": "1.0.1",
  "description": "React useasync timeout utils",
  "scripts": {
    "test": "jest",
    "build": "npm run validate && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs/",
    "lint": "tslint -p tsconfig.json",
    "validate": "npm test && npm run lint",
    "lint:fix": "tslint --fix -p tsconfig.json",
    "release": "npm run build && npm run release:public",
    "release:public": "npm publish --access public"
  },
  "author": "Dust in Est",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dustinest/typescript-async-utils.git"
  },
  "keywords": [
    "react",
    "hooks",
    "hook",
    "useasync",
    "promise",
    "useState",
    "future",
    "TypeScript"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dustinest/typescript-async-utils/issues"
  },
  "homepage": "https://github.com/dustinest/typescript-async-utils#readme",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "babel-jest": "latest",
    "jest": "^28.0.3",
    "jest-environment-jsdom": "^28.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.1",
    "ts-jest": "^28.0.1",
    "tslint": "^6.1.3",
    "typescript": "^4.6.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "fakeTimers": {"enableGlobally": true}
  },
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "sideEffects": false
}
